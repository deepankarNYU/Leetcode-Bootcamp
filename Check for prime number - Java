import java.util.Scanner;

public class Main {

    public static void main(String[] args) {



	 Scanner scn = new Scanner(System.in);
	 int n = scn.nextInt();

        int counter = 2;
        boolean flag = true;
        while (counter <= n-1) {
            if (n%counter == 0) {
                flag = false;
            }
            counter=counter+1;
        }
        if (flag == true){
            System.out.println("the number is prime");

        }
        else {
            System.out.println("The number is not prime");
        }
    }

    }
