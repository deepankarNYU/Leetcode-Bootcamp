

import java.util.Scanner;

public class Main<arr> {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();

        Reverse(n);
    }



    public static void Reverse (int a) {
        int ans = 0;
        while (a>0){
            int last= a%10;
            ans = ans*10 + last;
            a/=10;
        }
        System.out.println(ans);

    }



}














