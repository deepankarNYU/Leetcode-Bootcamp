
//Reverse a stack

import java.util.*;

public class Main {

    public static void main(String[] args) {

        Stack <Integer> s1 = new Stack<>(); //stack to be reversed

        s1.push(1);
        s1.push(2);
        s1.push(3);

        Stack <Integer> helper = new Stack<>(); //helper stack

        while (!s1.isEmpty()){
            helper.push(s1.pop());
        }

        while (!helper.isEmpty()){
           System.out.println(helper.pop());
        }
        
    }

}



