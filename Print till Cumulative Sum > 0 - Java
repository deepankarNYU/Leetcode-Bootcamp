

import java.util.Scanner;

public class Main<arr> {

    public static void main(String[] args) {
        

        int [] arr1 = {1 ,2 ,88, -100 ,49}; //sample input

        int sum=0;

        for (int i=0;i<arr1.length;i++){

            sum=sum+arr1[i];
            if (sum>0){
                System.out.println(arr1[i]);
            }
            else {
                break;
            }
        }
    }

  
}
















